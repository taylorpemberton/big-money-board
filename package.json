{
  "name": "big-money-board",
  "version": "1.0.0",
  "homepage": "https://<your-github-username>.github.io/<your-repo-name>",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "ts-node server/index.ts",
    "dev": "vite",
    "predeploy": "npm run build",
    "deploy": "npm run build && git add dist && git commit -m 'Deploy to main' && git subtree push --prefix dist origin main",
    "build:css": "node ./node_modules/.bin/tailwindcss -i ./src/tailwind.css -o ./src/styles/output.css",
    "watch:css": "node ./node_modules/.bin/tailwindcss -i ./src/tailwind.css -o ./src/styles/output.css --watch"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.8",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "vite": "^6.2.0"
  },
  "proxy": "http://localhost:3001",
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "stripe": "^17.7.0",
    "use-sound": "^5.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2"
  }
}
